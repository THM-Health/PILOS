{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "anlutro/l4-settings": "^1.0.0",
        "directorytree/ldaprecord-laravel": "^3.0",
        "doctrine/dbal": "^3.0",
        "goldspecdigital/laravel-eloquent-uuid": "^10",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.21",
        "laravel/prompts": "^0.1.13",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^v3.2",
        "laravel/telescope": "^4.15",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.3",
        "littleredbutton/bigbluebutton-api-php": "^5.3.0",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/collision": "^7.0",
        "spatie/laravel-ignition": "^2.0",
        "symfony/var-exporter": "^6.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "3.*",
        "mockery/mockery": "^1.4.2",
        "php-coveralls/php-coveralls": "^2.3.0",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "3.*",
        "timacdonald/log-fake": "^2.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
          "php": "8.1"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "directorytree/ldaprecord-laravel",
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/",
          "Database\\Factories\\": "database/factories/",
          "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "test": "php artisan test --parallel",
        "test-ci": "php artisan test --parallel --testsuite=Unit,Feature",
        "test-integration": "php artisan test --parallel --testsuite=Integration",
        "fix-cs": "php-cs-fixer fix --allow-risky=yes"

    }
}
