{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "anlutro/l4-settings": "^1.0.0",
        "bensampo/laravel-enum": "^6.0",
        "directorytree/ldaprecord-laravel": "^2.7",
        "doctrine/dbal": "^3.0",
        "goldspecdigital/laravel-eloquent-uuid": "^10",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel-lang/lang": "~12.21",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^v3.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "littleredbutton/bigbluebutton-api-php": "^4.2.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "barryvdh/laravel-ide-helper": "^2.7",
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "3.*",
        "laravel/sail": "^1.14",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "php-coveralls/php-coveralls": "^2.3.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-exporter": "^6.2",
        "timacdonald/log-fake": "^2.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
          "php": "8.1"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "directorytree/ldaprecord-laravel"
            ]
        }
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/",
          "Database\\Factories\\": "database/factories/",
          "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "php artisan test --parallel",
        "fix-cs": "php-cs-fixer fix --allow-risky=yes"

    }
}
